{
    <script>
        $(function () { toastr.warning('@TempData[TouristTourGuide.Common.NotificationMessage.WarningMassage].ToString()'); });
    </script>
}

@if (TempData[TouristTourGuide.Common.NotificationMessage.SuccessMassage] != null)
{
    <script>
        $(function () { toastr.success('@TempData[TouristTourGuide.Common.NotificationMessage.SuccessMassage].ToString()'); });
    </script>
}
