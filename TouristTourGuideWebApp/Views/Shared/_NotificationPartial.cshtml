@if (TempData[TouristTourGuide.Common.NotificationMessage.ErrorMassage] != null)
{
    <script>
        $(function () { toastr.error('@TempData[TouristTourGuide.Common.NotificationMessage.ErrorMassage].ToString()'); });
    </script>
}

